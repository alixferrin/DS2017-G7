<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<AnchorPane id="AnchorPane" prefHeight="680.0" prefWidth="877.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.cliente.ClienteController">
   <children>
      <MenuBar fx:id="menuBar" layoutY="1.0" prefHeight="25.0" prefWidth="1001.0">
        <menus>
          <Menu fx:id="menuClategorias" mnemonicParsing="false" text="Categorias">
            <items>
              <MenuItem fx:id="menuListCategoria" mnemonicParsing="false" onAction="#showListarCategoriasCLIE" text="Listar Categorias" />
            </items>
          </Menu>
          <Menu fx:id="menuBuscar" mnemonicParsing="false" text="Busqueda">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#showBuscar" text="Buscar Platillo" />
               </items></Menu>
          <Menu fx:id="menuLogOut" mnemonicParsing="false" text="Cerrar ...">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#cerrarSesion" text="Cerrar Sesión" />
                  <MenuItem mnemonicParsing="false" onAction="#cerrar" text="Cerrar Aplicación" />
               </items></Menu>
        </menus>
      </MenuBar>
      <StackPane fx:id="loaderPane" layoutY="26.0" prefHeight="654.0" prefWidth="1001.0" />
   </children>
</AnchorPane>
